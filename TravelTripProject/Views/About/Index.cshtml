<div class="content">
    <div class="wrap">
        <div class="about">
            <h3>Hakkımızda</h3>
            @foreach (var item in Model)
            {
                <img src="@item.FotoUrl" alt="" />

                <p>@item.Description</p>
            }
        </div>
    </div>
</div>
